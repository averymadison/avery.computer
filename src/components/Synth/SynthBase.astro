<style>
  .synth-base {
    fill: var(--gray-4);
    stroke: var(--gray-12);
    stroke-width: 3px;
    vector-effect: non-scaling-stroke;
  }

  :global(.transitioning) .synth-base {
    transition:
      fill 0.2s ease-in-out,
      stroke 0.2s ease-in-out;
  }
</style>
