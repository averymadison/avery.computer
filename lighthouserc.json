{
  "ci": {
    "assert": {
      "preset": "lighthouse:no-pwa",
      "assertions": {
        "bf-cache": "off",
        "csp-xss": "off",
        "font-display": "off",
        "uses-rel-preconnect": "off",
        "uses-long-cache-ttl": "off",
        "unused-javascript": ["warn", { "maxLength": 2 }]
      }
    },
    "collect": {
      "staticDistDir": ".vercel/output/static/"
    },
    "upload": {
      "target": "temporary-public-storage"
    }
  }
}
